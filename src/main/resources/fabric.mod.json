{
    "schemaVersion": 1,
    "id": "screenshotutils",
    "version": "${version}",
    "name": "ScreenshotUtils",
    "description": "Adds some more functionality to screenshots",
    "authors": [
        "Squagward"
    ],
    "contact": {
        "homepage": "https://github.com/camnwalter/ScreenshotUtils",
        "sources": "https://github.com/camnwalter/ScreenshotUtils",
        "issues": "https://github.com/camnwalter/ScreenshotUtils/issues"
    },
    "license": "CC0-1.0",
    "icon": "assets/screenshotutils/icon.png",
    "environment": "client",
    "entrypoints": {
        "client": [{
            "adapter": "kotlin",
            "value": "com.squagward.screenshotutils.ScreenshotUtils"
        }]
    },
    "mixins": [
        "screenshotutils.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.14.21",
        "minecraft": "~1.20.1",
        "java": ">=17",
        "fabric-api": "*",
        "fabric-language-kotlin": ">=${fabric_kotlin_version}"
    }
}
